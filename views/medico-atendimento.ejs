<section class="modals">
	<div class="modal" id="summary">
		<div class="modal-box">
			<div class="modal-title">
				<h2>Resumo do Paciente</h2>
			</div>
			<% if(!attendance.length) { %>
				<p>Paciente sem consultas anteriores</p>
			<% } else { %>
				<div class="timeline">
					<% for(let i = 0; i < attendance.length; i++) { %>
						<% if(attendance[i].type === 'presencial') { %>
							<% if(attendanceNow && attendance[i]._id != attendanceNow._id) { %>
								<% if(i % 2 == 0) { %>
									<div class="timeline-item">
										<div class="timeline-time"><script>document.write(moment('<%= attendance[i].createdAt %>', 'DD/MM/YYYY h:mm:ss').format('L'))</script></div>
										<div class="timeline-content">
											<p><strong>Pontuário</strong></p>
											<a href="/atendimento/<%= attendance[i]._id %>">Clique pra ver</a>
										</div>
									</div>
								<% } else { %>
									<div class="timeline-item timeline-ltd">
										<div class="timeline-time"><script>document.write(moment('<%= attendance[i].createdAt %>', 'DD/MM/YYYY h:mm:ss').format('L'))</script></div>
										<div class="timeline-content">
											<p><strong>Pontuário</strong></p>
											<a href="/atendimento/<%= attendance[i]._id %>">Clique pra ver</a>
										</div>
									</div>
								<% } %>
							<% } else { %>
								<% if(i % 2 == 0) { %>
									<div class="timeline-item">
										<div class="timeline-time"><script>document.write(moment('<%= attendance[i].createdAt %>', 'DD/MM/YYYY h:mm:ss').format('L'))</script></div>
										<div class="timeline-content">
											<p><strong>Pontuário</strong></p>
											<a href="/atendimento/<%= attendance[i]._id %>">Clique pra ver</a>
										</div>
									</div>
								<% } else { %>
									<div class="timeline-item timeline-ltd">
										<div class="timeline-time"><script>document.write(moment('<%= attendance[i].createdAt %>', 'DD/MM/YYYY h:mm:ss').format('L'))</script></div>
										<div class="timeline-content">
											<p><strong>Pontuário</strong></p>
											<a href="/atendimento/<%= attendance[i]._id %>">Clique pra ver</a>
										</div>
									</div>
								<% } %>
							<% } %>
						<% } %>
					<% } %>
				</div>
			<% } %>
		</div>
	</div>	

